{
  "version": 2,
  "functions": {
    "vercel_wsgi.py": {
      "runtime": "python3.9",
      "maxLambdaSize": "15mb"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "backend.settings"
  }
}
